                    <local:CircleWidget x:Name="circleWidget_auto7" HorizontalAlignment="Left" VerticalAlignment="Top" Angle="{Binding Para.Angle7, Mode=TwoWay}" Margin="765,172,0,0" Height="50" Width="50"/>
